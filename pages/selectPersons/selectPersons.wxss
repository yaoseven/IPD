.titleManage{
  display: flex;
  flex-direction: column;
  width:100%;
  height: 100%;
}

.person-box {
  flex: 1;
  overflow: auto;
}

.person{
  display: flex;
  height: 100rpx;
  padding: 0 30rpx;
  align-items: center;
  font-size: 28rpx;
}
.personImg{
  width:60rpx;
  height: 60rpx;
  margin-right: 30rpx;
}
.personImg image{
  width:100%;
  height: 100%;
  border-radius: 50%;
}

.personName{
  flex: 1;
}

.isconfirm{
  display: flex;
  height: 84rpx;
  line-height: 84rpx;
  justify-content: space-between;
  background: #181C29;
  color: #ffffff;
  font-size: 28rpx;
  padding: 0 30rpx;
}

.changebox{
  display: flex;
  align-items: center;
  justify-content: center;
  border: 2rpx solid #ccc;
  width: 34rpx;
  height: 34rpx;
  border-radius: 50%;
}